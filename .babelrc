{
  "presets": [
    ["env", {
      "targets": {
        "node": "current"
      }
    }],
    "es2015",
    "react"
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    "transform-object-rest-spread",
    "transform-class-properties"
  ]
}
